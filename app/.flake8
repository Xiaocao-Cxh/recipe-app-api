[flake8]
exclude =
    migrations,
    __pycache__,
    manage.py,
    settings.py,
# ask flake8 to ignore these errors when linting
# migrations are autogenerated by Django and should not be checked
# __pycache__ is autogenerated by Python and should not be checked
# manage.py is a script file that Django includes
# settings.py is a Django settings file